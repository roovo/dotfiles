[apply]
  whitespace    = nowarn
[color]
  branch        = auto
  diff          = auto
  interactive   = auto
  status        = auto
[core]
  editor        = vim
  excludesfile  = ~/.gitignore
  quotepath     = false
[github]
  user          = roovo
[help]
  autocorrect   = 1
[merge]
  summary       = true
  tool          = opendiff
[push]
  default       = current
[rerere]
  enabled       = 1
[user]
  email         = GIT_AUTHOR_EMAIL
  name          = GIT_AUTHOR_NAME
